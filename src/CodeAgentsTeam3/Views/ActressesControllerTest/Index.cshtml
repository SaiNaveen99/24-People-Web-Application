@model IEnumerable<CodeAgentsTeam3.Models.Actress>

@{
    ViewData["Title"] = "Index";
}

<h2>Index</h2>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Achievements)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DateOfBirth)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.EducationQualification)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FirstdName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Language)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.LastName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Place)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Remuneration)
        </th>
        <th></th>
    </tr>
    
@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Achievements)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DateOfBirth)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.EducationQualification)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FirstdName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Language)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.LastName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Place)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Remuneration)
        </td>
        <td>
            <a asp-action="Edit" asp-route-id="@item.ActressID">Edit</a> |
            <a asp-action="Details" asp-route-id="@item.ActressID">Details</a> |
            <a asp-action="Delete" asp-route-id="@item.ActressID">Delete</a>
        </td>
    </tr>
}
</table>
